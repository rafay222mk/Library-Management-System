<html>
<head>
  <title>Student Page</title>
</head>
<body>
<div class="container">
  <table class="table table-striped table-dark">
    <thead>
    <th>User ID</th>
    <th>User Name</th>
    <th>Email</th>
    <th>Role</th>
    </thead>
    <tbody>
    <% @std.each do |user| %>
      <td><%= user.id %></td>
      <td><%= user.user_name %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
</body>
</html>

